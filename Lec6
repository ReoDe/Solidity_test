pragma solidity ^0.4.24;

contract View_example
{
     uint256 public a = 1;
    
    function read_a() public view returns(uint256){
        return a+2;
    } 
}

contract Pure_example
{
    
    function read_a() pure public returns(uint256)
    {
        uint256 a = 3;
        return a+2;
    } 
}
